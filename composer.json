{
    "name": "kyrill/php-di-container",
    "description": "A php dependency injection container",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kyrill\\PhpDiContainer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Kyrill van Fessem",
            "email": "kyrill.van.fessem@maxserv.com"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^12.2",
        "friendsofphp/php-cs-fixer": "^3.85"
    }
}
